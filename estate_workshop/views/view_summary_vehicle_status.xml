        <record model='ir.ui.view' id='view_summary_vehicle_available_search'>
            <field name="name">view.summary.vehicle.available</field>
            <field name="model">view.summary.vehicle.available</field>
            <field name="arch" type="xml">
                <search string="Vehicles Summary " >
                    <field name="year_log_text" string="Year"/>
                    <field name="month_log_text" string="Month"/>
                    <field name="vehicle_id" string="Vehicle"/>
                    <field name="total_day_available" string="Available"/>
                    <field name="total_day_breakdown" string="Breakdown"/>
                    <field name="total_day_standby" string="Standby"/>
                    <field name="total_downtime" string="Downtime"/>
                    <group expand="0" string="Group By">
                        <filter name="groupby_vehicle" context="{'group_by' : 'vehicle_id'}" string="Vehicle"/>
                        <filter name="groupby_month" context="{'group_by' : 'month_log_text'}" string="Month"/>
                        <filter name="groupby_year" context="{'group_by' : 'year_log_text'}" string="Year"/>
                    </group>
                </search>
            </field>
