<openerp>
    <data>
         <record model="ir.ui.view" id="view_form_view_goods_report_in_tree">
             <field name="name">Goods Report In Tree</field>
             <field name="model">view.goods.in.report</field>
             <field name="priority" eval="1"/>
             <field name="arch" type="xml">
                <tree>
                    <field name="id" string="ID" invisible="1"/>
                    <field name="complete_name_picking" string="Picking No"/>
                    <field name="backorder_id" string="Back Order" readonly="1"/>
                    <field name="purchase_order_name" string="PO No" readonly="1"/>
                    <field name="move_date" string="Date"/>
                    <field name="product_id" string="Product"/>
                    <field name="picking_id" string="Picking" invisible="1"/>
                    <field name="product_uom" string="UOM"/>
                    <field name="product_uom_qty" string="QTY"/>
                    <field name="price_unit" string="Price Unit"/>
                    <field name="total" string="Total"/>
                </tree>
            </field>
         </record>

         <record id="view_form_view_goods_report_in_form" model="ir.ui.view">
            <field name="name">Goods Report In Form</field>
            <field name="model">view.goods.in.report</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <form>
                     <sheet>
                         <group>
                            <field name="id" string="ID" invisible="1"/>
                            <field name="complete_name_picking" string="Picking No" readonly="1"/>
                            <field name="backorder_id" string="Back Order" readonly="0"/>
                            <field name="purchase_order_name" string="PO No" readonly="1"/>
                            <field name="move_date" string="Date"/>
                         </group>
                         <group>
                            <field name="product_id" string="Product" readonly="1"/>
                            <field name="picking_id" string="Picking" invisible="0" readonly="1"/>
                            <field name="product_uom" string="UOM" readonly="1"/>
                            <field name="product_uom_qty" string="QTY" readonly="1"/>
                            <field name="price_unit" string="Price Unit" readonly="1"/>
                            <field name="total" string="Total" readonly="1"/>
                         </group>
                     </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="goods_report_in_action">
            <field name="name">Goods In Reports</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">view.goods.in.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context"></field>
            <!--<field name="search_view_id" ref="view_quotation_comparison_search"/>-->
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to start a new report.
              </p><p>

              </p>
            </field>
        </record>

        <menuitem
            id="menu_goods_report_in_act"
            sequence="4"
            parent="stock.menu_warehouse_report"
            action="goods_report_in_action"
            />

    </data>
</openerp>