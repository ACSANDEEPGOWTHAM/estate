<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Product Category-->
        <record id="product_uom_categ_area" model="product.uom.categ">
            <field name="name">Area</field>
        </record>

        <!-- Product UOM -->
        <record id="product_uom_hectare" model="product.uom">
            <field name="category_id" ref="product_uom_categ_area"/>
            <field name="name">ha</field>
            <field name="factor" eval="1.0"/>
        </record>

        <record id="product_uom_bag" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">bag</field>
        </record>

        <record id="product_uom_bibit" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">bbt</field>
        </record>

        <record id="product_uom_kecambah" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">kcb</field>
        </record>

        <record id="product_uom_lubang" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">lbg</field>
        </record>

        <record id="product_uom_pokok" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">pkk</field>
        </record>

        <record id="product_uom_tp" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">tp</field>
        </record>

        <!-- Decimal Precision -->
        <record id="decimal_estate" model="decimal.precision">
            <field name="name">Estate</field>
            <field name="digits">6</field>
        </record>
        <record id="decimal_standard_price" model="decimal.precision">
            <field name="name">Standard Price</field>
            <field name="digits">6</field>
        </record>
    </data>
</openerp>
