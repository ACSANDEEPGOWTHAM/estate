<?xml version="1.0" encoding="utf-8"?>
<!--Custom report.-->
<openerp>
    <data>
        <template id="report_reportplantation_document">
            <t t-call="report.external_layout">
                <div class="page">
                    <h2 colors="black">
                        <span >Rekapitulasi seleksi bibit Pre Nursery</span>
                    </h2>

                </div>
            </t>
        </template>
        <template id="reportplantation_report">
            <t t-call="report.html_container">
                <t t-foreach="doc_ids" t-as="doc_id">
                    <t t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang',
                    'estate_nursery.report_plantation_document')"/>
                </t>
            </t>
        </template>
    </data>
</openerp>