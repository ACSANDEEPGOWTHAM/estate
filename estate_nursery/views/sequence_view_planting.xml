<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Sequences for planting code -->
        <record id="plt_code_sequence" model="ir.sequence.type">
            <field name="name">Seed Delivery Order</field>
            <field name="code">estate.nursery.seeddo</field>
        </record>

        <record id="seq_planting" model="ir.sequence">
            <field name="name">Seed Delivery Order</field>
            <field name="code">estate.nursery.seeddo</field>
           <field name="prefix">SDO</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for request code -->
        <record id="rsq_code_sequence" model="ir.sequence.type">
            <field name="name">Seed Request </field>
            <field name="code">estate.nursery.request</field>
        </record>

        <record id="seq_request_seed" model="ir.sequence">
            <field name="name">Seed Request</field>
            <field name="code">estate.nursery.request</field>
           <field name="prefix">BPB</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- End of Sequence code for request code -->

    </data>
</openerp>