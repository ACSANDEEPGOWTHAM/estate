<openerp>
    <data>
        <record model="ir.ui.view" id="view_tree_report plantation">
            <field name="name">Report Plantation</field>
            <field name="model">estate.nursery.reportplantation</field>
            <field name="arch" type="xml">
                <tree string="Nursery selection View">
                    <field name="names"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="view_form_report_plantation">
            <field name="name">report plantation</field>
            <field name="model">estate.nursery.reportplantation</field>
            <field name="arch" type="xml">
                <form String="Nursery selection view">
                    <group>
                        <group >
                            <field name="names"/>
                            <field name="partner_id" invisible="1"/>
                            <field name="report_date"/>
                        </group>
                        <group >
                           <!-- <page string="Batch">
                                <field name="batch_ids"/>
                            </page>
                            <page string="Selection">
                                <field name="selection_ids"/>
                            </page>-->
                        </group>
                    </group>
                </form>
            </field>
        </record>

         <record model="ir.actions.act_window" id="action_list_reportplantation">
            <field name="name">Report Plantation</field>
            <field name="res_model">estate.nursery.reportplantation</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="nursery_line_report_plantation" name="Report Batch and Selection"
                  parent="estate_nursery_menu"
                  action="action_list_reportplantation"
                  sequence="16"
                />

    </data>
</openerp>