<openerp>
    <data>
        <record id="paperformat_reportdiv" model="report.paperformat">
            <field name="name">Report Seed Received</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">600</field>
            <field name="page_width">175</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">50</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">3</field>
            <field name="margin_right">3</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">30</field>
            <field name="dpi">80</field>
        </record>
        <report
            id="custom_reportdiv_report_plantation"
            model="estate.nursery.reportline"
            string="Report Pre Nursery div"
            report_type="qweb-pdf"
            name="estate_nursery.reportplantation_div_report"
            attachment_use="False"
            file="estate_nursery.reportplantation_div_report"
        />
        <record id="custom_reportdiv_report_plantation" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="estate_nursery.paperformat_reportdiv"/>
        </record>
    </data>
</openerp>